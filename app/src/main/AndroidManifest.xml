<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.anheew1kookmin.exhibition">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>

            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan"
            android:saveEnabled="true">
        </activity>
        <activity android:name=".UploadActivity"></activity>
        <activity android:name=".RegisterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".SubArtworkActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".SubPlaceActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".PurchaseActivity"
            android:screenOrientation="portrait"></activity>
    </application>
</manifest>